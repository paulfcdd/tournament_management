{% extends 'layouts/view.html.twig' %}

{% block url_to_form %}
{{ urlToForm }}
{% endblock %}

{% block country_name %}
    {{ country.name }}
{% endblock %}

{% block leagues_table_body %}
    {% for league in leagues %}
        <tr>
            <td>{{ league.id }}</td>
            <td>{{ league.name }}</td>
            <td>{{ league.leagueRanking }}</td>
            <td>{{ league.numberOfClubs }}</td>
            <td>0</td>
        </tr>
    {% endfor %}
{% endblock %}